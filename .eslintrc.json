{
    "extends": "airbnb-base",

    "rules": {
      "semi": ["off"],
      "padded-blocks": ["off"],
      "object-curly-spacing": ["off"],
      "keyword-spacing": ["off"],
      "no-console": ["off"],
      "jsx-quotes": ["warn", "prefer-single"],
      "curly": ["warn", "multi-or-nest"],
      "arrow-spacing": ["warn"]
    }
}
